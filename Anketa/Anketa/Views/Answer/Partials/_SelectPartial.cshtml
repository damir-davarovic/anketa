@model Anketa.Models.AnswerModels.AnswerChoiceMultiple
@{
    ViewBag.FalseBool = false;
}
	<input id=@Html.Raw(Model.choiceId) type="checkbox" class="disabled" value=@Html.Raw(Model.choiceId)/>
	<label for=@Html.Raw(Model.choiceId)>
		 @Html.EditorFor(model => model.choiceText, new { htmlAttributes = new { @class = "form-control" , placeholder = "Choice will look like this" } })  
	</label>
